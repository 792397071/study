#define _CRT_SECURE_NO_WARNINGS 1

#include <stdio.h>

typedef struct stu
{
	char name[20];
	short age;
	char tele[12];
	char sex[5];
	char* pc;
}Stu;



int main()
{
	char arr[] = { "hello world" };
	Stu s1={ "张三",20, "13886868666","男" ,arr};
	Stu s2 = { "旺财",11, "13886868667","女" ,arr};
	printf("%s\n", s1.pc);
	return 0;

}
