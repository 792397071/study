int mystrlen(char* a)
{
	char* start = a;
	char* end = a;
	while (*end != '\0')
	{
		end++;
	}
	return end-start;


}

int main()
{
	char arr[] = "asdsafad";
	int len = mystrlen(arr);
	printf("%d\n", len);

}


