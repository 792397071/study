#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>
#include<windows.h>
#include<stdlib.h>
int main()
{
	char password[20];
		int i;
		for (i = 0; i < 3; i++)
		{
			printf("your password:");
			scanf("%s", password);
			if (strcmp(password, "12345678") == 0)
			{
				printf("its ok\n");
				break;
			}
			else
				printf("wrong\n");

		}
		if (i == 3)
			printf("byebye\n");




}
