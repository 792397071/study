int main()
{
	char passward[20] = { 0 };
	int ret;
	printf("请输入密码->");
	scanf("%s", passward);
	while (getchar() != '\n');
	{

		;

	}
	printf("请确定正确与否（Y/N）");
	ret=getchar();
	if (ret = 'Y')
	{
		printf("yes,ok\n");
	}
	else
		printf("nooooooo\n");

